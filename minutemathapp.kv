#:import RiseInTransition kivy.uix.screenmanager.RiseInTransition
ScreenManager: #Use ScreenManager to swipe screens 
	transition: RiseInTransition()
	#------------------------------------------------------------------------------------------------------------------------------------------------------
	
	Screen:
		name: 'Welcome Screen'

		canvas:


			Rectangle:
				source: 'mad_minute_image.jpg'
				pos: self.pos
				size: self.size
	#---------------------------------------------------------------------------------------------------------------------------------------------------------

	Screen:
		name: 'Options Screen'

		canvas:

			Rectangle:
				source: 'multi_color.jpg'
				size: self.size
				pos: self.pos

		GridLayout:
			rows: 4
			cols: 1

			Button:
				text: 'Easy Math Level'
				background_normal: 'navy.png'
				on_press: app.root.current = 'Easy Math Options'

			Button: 
				text: 'Intermediate Math Level'
				background_normal: 'navy.png'
				on_press: app.root.current = 'Intermediate Math Options'

			Button:
				text: 'Expert Math Level'
				background_normal: 'navy.png'
				on_press: app.root.current = 'Expert Math Options'

			Button:
				text:'About Page'
				background_normal: 'navy.png'
				on_press: app.root.current = 'About Screen'
	#-------------------------------------------------------------------------------------------------------------------------------------------------
	Screen:
		name: 'Easy Math Options'

		GridLayout:
			rows: 5
			cols: 1

			Button:
				text: 'Easy Addition'
				background_normal: 'navy.png'
				on_press: app.root.current = 'Easy Addition Level'

			Button:
				text: 'Easy Subtraction'
				background_normal: 'navy.png'
				on_press: app.root.current = 'Easy Subtraction Level'

			Button:
				text: 'Easy Multiplication'
				background_normal: 'navy.png'
				on_press: app.root.current = 'Easy Multiplication Level'

			Button:
				text: 'Easy Division'
				background_normal: 'navy.png'
				on_press: app.root.current = 'Easy Division Level'

			Button:
				text: 'Go Back'
				background_normal: 'navy.png'
				on_press: app.root.current = 'Options Screen'
	#-----------------------------------------------------------------------------------------------------------------------------------------------------

	Screen:
		name: 'Intermediate Math Options'

		GridLayout:
			rows: 5
			cols: 1

